//CLASE CLIENTE


public class Cliente {
    int dni;
    int numcliente;
    String nombre;
    Cuenta cuentas;

    int saldo;
    int numcuenta;

public Cliente (int dni, int numcliente, String nombre, Cuenta cuentas) {
    this.dni = dni;
    this.numcliente = numcliente;
    this.nombre = nombre;
    this.cuentas = cuentas;

}

public int getDni(){
    return dni;
    }

public int getNumcliente(){
    return numcliente;
}

public String getNombre(){
    return nombre;
}

public int getSaldo(){
    return saldo;
}

public int getNumcuenta(){
    return numcuenta;
}

}


