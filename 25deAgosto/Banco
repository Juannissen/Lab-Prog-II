}



//CLASE CLIENTE


public class Cliente {
    int dni;
    int numcliente;
    String nombre;

    int saldo;
    int numcuenta;

public void cliente (int dni, int numcliente, String nombre) {
    System.out.println("Ingrese su DNI");
    int cuentas = s.nextInt();

    System.out.println("Ingrese su num de cliente");

    System.out.println("Ingrese nombre");



    this.dni = dni;
    this.numcliente = numcliente;
    this.nombre = nombre;
}

public int getDni(){
    return dni;
    }

public int getNumcliente(){
    return numcliente;
}

public String getNombre(){
    return nombre;
}

public int getSaldo(){
    return saldo;
}

public int getNumcuenta(){
    return numcuenta;
}

}



